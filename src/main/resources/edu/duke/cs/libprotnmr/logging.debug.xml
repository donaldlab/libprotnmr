<?xml version="1.0" encoding="UTF-8"?>
<Configuration>

    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout>
                <pattern>[%-5p %c{1}.%M() line %L] %m%n</pattern>
            </PatternLayout>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="info">
            <AppenderRef ref="stdout"/>
        </Root>
    </Loggers>

</Configuration>